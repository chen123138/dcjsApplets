/**index.wxss**/


.worker-click{
  height: 120rpx;
  width: 120rpx;
  background-color: #42a5f5;
  border-radius: 100%;
  position: fixed;
  bottom: 150rpx;
  right: 20rpx;
  display: flex;
  z-index: 9;
}
.worker-click navigator{
  font-size: 32rpx;
  max-width: 80rpx;
  color: #fff;
  text-align: center;
}
.worker-click navigator image {
  width: 80rpx;
  height: 80rpx;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
}