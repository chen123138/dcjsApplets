<!--requestBuy/project/index.wxml-->
<view class="container">
  <block wx:for="{{projectList}}" wx:key="item">
    <view class="list_item" data-id='{{item.id}}' data-name="{{item.name}}" bindtap="goApplication">
      <text class="list_item_title">{{item.name}}</text>
    </view>
    <view class="space"></view>
  </block>
</view>